    <#assign prevArg=(viewIndex - 1)?string />
<a href="<@ofbizUrl>/main</@ofbizUrl>?viewIndex=${prevArg?if_exists}">Prev</a>
</#if>
<#if 0 < outputIndex >
 ${lowIndex?if_exists}-${outputIndex?if_exists}
<#else>
 No records found
