        <auto-attributes mode="IN" include="nonpk" optional="true">
            <exclude field-name="statusDateTime"/>
        </auto-attributes>
    </service>
    <service name="unassignPartyFromWorkEffort" default-entity-name="WorkEffortPartyAssignment" engine="simple"
