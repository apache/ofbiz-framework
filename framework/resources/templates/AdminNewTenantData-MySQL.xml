    <TenantDataSource tenantId="@tenantId@" entityGroupName="org.apache.ofbiz.olap"
        jdbcUri="jdbc:mysql://@db-IP@/ofbizolap_@tenantId@" jdbcUsername="@db-User@" jdbcPassword="@db-Password@"/>
</entity-engine-xml>