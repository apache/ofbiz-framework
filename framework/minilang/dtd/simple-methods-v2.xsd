            </xs:documentation>
        </xs:annotation>
        <xs:complexType mixed="true">
            <xs:attributeGroup ref="attlist.propertyInfoOptional"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="results-to-map">
        <xs:annotation>
            <xs:documentation>
