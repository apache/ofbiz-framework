<entity-engine-xml>
  <StatusItem sequenceId="01" description="Ready to be shipped" statusCode="PURCH_SHIP_READY"
    statusId="PURCH_SHIP_READY" statusTypeId="SHIPMENT_STATUS"/>

  <!-- valid status changes -->
  <StatusValidChange statusId="PURCH_SHIP_CREATED" statusIdTo="PURCH_SHIP_READY"
    transitionName="Drop shipment packed"/>
  <StatusValidChange condition="" statusId="PURCH_SHIP_READY" statusIdTo="PURCH_SHIP_RECEIVED"
    transitionName="Receive"/>
  <StatusValidChange condition="" statusId="PURCH_SHIP_READY" statusIdTo="PURCH_SHIP_SHIPPED"
    transitionName="Ship"/>

  <!-- supplier POC role names -->
  <EnumerationType description="Supplier POC Role Name" enumTypeId="SPLR_POC_RL" hasTable="N"
    parentTypeId=""/>
  <Enumeration description="Supplier Point of Contact" enumCode="SPLR_POC_RL_01"
    enumId="SPLR_POC_RL_01" sequenceId="01" enumTypeId="SPLR_POC_RL"/>
  <Enumeration description="Area Sales Rep / Product Trainer" enumCode="SPLR_POC_RL_02"
    enumId="SPLR_POC_RL_02" sequenceId="02" enumTypeId="SPLR_POC_RL"/>

  <!-- Application related user preference -->
  <UserPrefGroupType userPrefGroupTypeId="APPL_PREFERENCES" description="Application preferences"/>
  <EnumerationType description="Application preferences" enumTypeId="APPL_PREFERENCES" hasTable="N"
    parentTypeId="USER_PREF_GROUPS"/>
  <Enumeration description="Application Sidebar State (Collapsed/Open)" enumCode=""
