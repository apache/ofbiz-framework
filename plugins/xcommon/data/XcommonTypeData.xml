    enumCode="History" enumId="HISTORY" sequenceId="01" enumTypeId="STRENGTH"/>
  <Enumeration
    description="Our competitors are pushing boxes.But we know systems,networks,programming and data management."
    enumCode="Knowledge" enumId="KNOWLEDGE" sequenceId="02" enumTypeId="STRENGTH"/>
  <Enumeration description="We get to know our customers, one by one."
    enumCode="Relationship Selling" enumId="RELATIONSHIP" sequenceId="02" enumTypeId="STRENGTH"/>

  <EnumerationType description="Swot Weakness" enumTypeId="WEAKNESS" hasTable="N" parentTypeId=""/>
  <Enumeration
