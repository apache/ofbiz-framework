  <!--Default Supplier Message Broadcast Template html-->
  <DataResource
    dataResourceId="CM_BRDCST_MSG"
    dataResourceName="Default HTML email message for CM message broadcast functionality."
    dataResourceTypeId="ELECTRONIC_TEXT"
    isPublic="Y"
